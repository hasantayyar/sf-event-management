{% extends '::base_admin.html.twig' %}

{% block content -%}
    <h1>Phone</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ entity.type }}</td>
            </tr>
            <tr>
                <th>Fullnumber</th>
                <td>{{ entity.fullNumber }}</td>
            </tr>
            <tr>
                <th>Countrycode</th>
                <td>{{ entity.countryCode }}</td>
            </tr>
            <tr>
                <th>Ismobile</th>
                <td>{{ entity.isMobile }}</td>
            </tr>
        </tbody>
    </table>

    <ul class="record_actions">
        <li>
            <a href="{{ path('admin_phone') }}">
                Back to the list
            </a>
        </li>
        <li>
            <a href="{{ path('admin_phone_edit', { 'id': entity.id }) }}">
                Edit
            </a>
        </li>
        <li>{{ form(delete_form) }}</li>
    </ul>
{% endblock %}
