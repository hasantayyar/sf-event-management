{% extends '::base_admin.html.twig' %}

{% block content -%}
    <h1>Transfer</h1>

    <table class="record_properties table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Destinationpointid</th>
                <td>{{ entity.destinationPointId }}</td>
            </tr>
            <tr>
                <th>Departurepointid</th>
                <td>{{ entity.departurePointId }}</td>
            </tr>
            <tr>
                <th>Venueid</th>
                <td>{{ entity.venueId }}</td>
            </tr>
            <tr>
                <th>Carid</th>
                <td>{{ entity.carId }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_transfer') }}">
        Back to the list
    </a> 
    <a href="{{ path('admin_transfer_edit', { 'id': entity.id }) }}">
        Edit
    </a>
    <hr>
    {{ form(delete_form) }} 
{% endblock %}
